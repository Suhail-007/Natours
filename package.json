{
  "name": "natours",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "complie:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concate.css css/style.comp.css css/icon-font.css",
    "post:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style compressed",
    "build:css": "npm-run-all complie:sass concat:css post:css compress:css"
  },
  "author": "Suhail-007@github.com",
  "license": "ISC",
  "description": "Landing page for natours",
   "devDependencies": {
     "autoprefixer": "^10.4.13",
     "concat": "^1.0.3",
     "sass": "^1.56.1",
     "npm-run-all": "^4.1.5",
     "postcss-cli": "^10.0.0"
   }
}
